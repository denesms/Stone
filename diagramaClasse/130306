        //============== Pagina√ß√£o ==============  

        $itens_por_pagina = 10;
        @$idcurso = $_GET['id'];
        @$pagina = $DAO->itemPorPagina($_GET['pagina'], $itens_por_pagina);
        $numPagina = $DAO->numTotal($itens_por_pagina, $idcurso);
        
        @$nomecurso = $_GET['nomecurso'];
        
        if (isset($idcurso)) {

            $conteudo = $DAO->paginacaoByCurso($pagina, $numPagina, $itens_por_pagina, $idcurso);
